/** 
 * Multiplexor:
 * out = a if sel == 0
 *       b otherwise
 */

CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    // Put your code here:
    Not(in=sel,out=o1);
    And(a=o1,b=a,out=o2);
    And(a=sel,b=b,out=o3);
    Or(a=o2,b=o3,out=out);
}